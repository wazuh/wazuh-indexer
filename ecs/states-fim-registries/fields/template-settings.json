{
  "index_patterns": ["wazuh-states-fim-registries*"],
  "priority": 1,
  "template": {
    "settings": {
      "index": {
        "number_of_shards": "1",
        "number_of_replicas": "0",
        "refresh_interval": "5s",
        "query.default_field": [
          "agent.host.architecture",
          "agent.host.ip",
          "agent.id",
          "agent.name",
          "agent.version",
          "event.category",
          "registry.arch",
          "registry.gid",
          "registry.group",
          "registry.hash.md5",
          "registry.hash.sha1",
          "registry.hash.sha256",
          "registry.inode",
          "registry.mtime",
          "registry.owner",
          "registry.path",
          "registry.size",
          "registry.uid",
          "registry.data.type",
          "registry.value"
        ]
      }
    }
  }
}
