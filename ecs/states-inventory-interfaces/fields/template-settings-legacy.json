{
  "index_patterns": ["wazuh-states-interfaces*"],
  "order": 1,
  "settings": {
    "index": {
      "number_of_shards": "1",
      "number_of_replicas": "0",
      "refresh_interval": "5s",
      "query.default_field": [
        "agent.id",
        "agent.ip",
        "observer.ingress.interface.name",
        "observer.ingress.interface.alias"
      ]
    }
  }
}
